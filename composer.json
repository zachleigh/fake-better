{
    "name": "zachleigh/laravel-real-copy",
    "description": "Use real copy via Faker in any Laravel app.",
    "keywords": ["laravel", "faker", "real copy", "custom", "copy", "testing"],
    "homepage": "https://github.com/zachleigh/laravel-real-copy",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Zach Leigh",
            "email": "zachleigh@fastmail.jp"
        }
    ],
    "require": {
        "fzaninotto/faker": "^1.4"
    },
    "require-dev": {
        "laravel/laravel": "~5.6",
        "phpunit/phpunit": "^7.0"
    },

    "autoload": {
        "psr-4": {
            "LaravelRealCopy\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelRealCopy\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelRealCopy\\ServiceProvider"
            ]
        }
    }
}
